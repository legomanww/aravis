<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>Aravis.Native</id>
        <version>0.0.1</version>
        <title>Aravis native library for supported platforms</title>
        <authors>AravisProject</authors>
        <owners>AravisProject</owners>
        <description>Aravis native library for supported platforms</description>
        <tags>Aravis native library</tags>
        <projectUrl>https://github.com/legomanww/aravis</projectUrl>
        <repository type="git" url="https://github.com/legomanww/aravis" />
        <contentFiles>
            <files include="aravis-source.tgz" buildAction="None" copyToOutput="true"/>
        </contentFiles>
        <license type="expression">LGPL-2.1-or-later</license>
    </metadata>
    <files>
        <!-- ALL -->
        <file src="COPYING" target="LICENSE" />
        <!-- Linux ARM64 -->
        <!-- <file src="src/build-linux-arm64/*.so" target="runtimes/linux-arm64/native" /> -->
        <!-- Linux x64 -->
        <!-- <file src="src/build-linux-x64/*.so" target="runtimes/linux-x64/native" /> -->
        <!-- MacOS -->
        <file src="dist/build-macos/*.dylib" target="runtimes/osx/native" />
        <!-- Windows -->
        <file src="dist/build-win-x64/*.dll" target="runtimes/win-x64/native" />
    </files>
</package>